# Telegram Bot Token
# Get from @BotFather
BOT_TOKEN=your_telegram_bot_token_here

# Faucet API URL (Default - jangan ubah kecuali ada perubahan)
FAUCET_API=https://faucet.iopn.tech/api/faucet

# Auto Claim Interval (dalam jam)
# Default: 24 (24 jam)
CLAIM_INTERVAL=24

# Maximum wallets per create command
# Default: 50
MAX_WALLETS_PER_CREATE=50

# Delay between claims (dalam detik)
# Untuk avoid rate limit
# Default: 5
CLAIM_DELAY=5

# Enable notifications
# true = kirim notif setiap claim berhasil
# false = silent mode
ENABLE_NOTIFICATIONS=true

# Log level
# debug, info, warn, error
LOG_LEVEL=info
